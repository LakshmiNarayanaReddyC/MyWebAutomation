<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="AutomationFrameworkSuite">
	<listeners>
		<listener class-name="ExtentReportListeners.ExtentReporterNG"></listener>
	</listeners>
	<test thread-count="5" name="AutomationFrameworkSuite_chrome">
		<parameter name="BrowserName" value="Chrome"></parameter>
		<classes>
			<class name="TestCases.LoginTestcases" />
			<class name="TestCases.InventoryTestcases" />
			<class name="TestCases.LogoutTestcases" />

		</classes>
	</test> <!-- LoginTest -->
	<test thread-count="5" name="AutomationFrameworkSuite_edge">
		<parameter name="BrowserName" value="Edge"></parameter>
		<classes>
			<class name="TestCases.LoginTestcases" />
			<class name="TestCases.InventoryTestcases" />
			<class name="TestCases.LogoutTestcases" />

		</classes>
	</test>

</suite> <!-- AutomationFrameworkSuite -->
